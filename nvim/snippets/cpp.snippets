namespace fastio {
    constexpr int BUFMAX = 1 << 19 ;
    char buf[BUFMAX], *p1, *p2 ;

    inline char
    gc() {
        return p1 == p2 && (p2 = (p1 = buf) + fread(buf, 1, BUFMAX, stdin), p1 == p2) ? EOF : *p1++;
    }

    template <typename Tp> inline void
    in(Tp &x) {
        int f(0);
        x = 0;
        static char ch = gc() ;

        for (; ch < '0' || ch > '9'; ch = gc()) {
            f = ch == '-' || f ? 1 : 0;
        }

        for (; ch >= '0' && ch <= '9'; ch = gc()) {
            // 0 - 48
            x = x * 10 + (ch ^ 48);
        }

        x = f ? -x : x ;
    }

    template <typename T, typename ...Args> inline void
    in(T &a, Args &...args) {
        return in(a), in(args...);
    }
#undef getc
    inline void
    putc(const char ch) {
        static std::streambuf *outbuf = std::cout.rdbuf();
        outbuf -> sputc(ch);
    }
    template <typename Tp> inline void
    out(const char ch, Tp x) {
        static std::streambuf *outbuf = std::cout.rdbuf() ;
        static char stk[21];
        static int top = 0;
        x = x < 0 ? outbuf -> sputc('-'), -x : x ;

        if (!x) {
            return outbuf -> sputc('0'), outbuf -> sputc(ch), void();
        }

        while (x) {
            stk[++top] = x % 10 + '0', x /= 10 ;
        }

        while (top) {
            outbuf -> sputc(stk[top--]) ;
        }

        outbuf->sputc(ch) ;
    }

    template <typename Tp> inline void
    out(Tp x) {
        static std::streambuf *outbuf = std::cout.rdbuf() ;
        static char stk[21];
        static int top = 0;
        x = x < 0 ? outbuf -> sputc('-'), -x : x ;

        if (!x) {
            return outbuf -> sputc('0'), void();
        }

        while (x) {
            stk[++top] = x % 10 + '0', x /= 10 ;
        }

        while (top) {
            outbuf -> sputc(stk[top--]) ;
        }
    }

    template<typename T, typename ...Args> inline void
    out(const char ch, T a, Args ...args) {
        out(ch, a);
        out(ch, args...);
    }

    template<typename T, typename ...Args> inline void
    out(T a, Args ...args) {
        out(a);
        out(args...);
    }

}
