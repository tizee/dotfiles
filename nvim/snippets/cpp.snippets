snippet BSearch "binary search" b
int BSearch(${1:vector<int> &sorted_list},${2:int key}){
	int left = 0, right = sorted_list.size()-1;
	while(left <= right){
		int mid = left + (right - left)/2;
		int val = sorted_list[mid];
		if( val == key ){
			return mid;
		} else if (val < key){
			left = mid + 1;
		}else{
			right = mid - 1;
		}
	}
	return left - 1;
}
endsnippet

